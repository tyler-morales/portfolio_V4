:root{--color-tertiary: #66BB6A;--color-bg: #F5F5F7;--color-link: #1976D2;--color-gray:#6C6C6C}html{scroll-behavior:smooth}.u-mt{margin-top:50px}*{margin:0;padding:0;box-sizing:border-box}ul,li{list-style-type:none}a{text-decoration:none;cursor:pointer}button{display:inline-block;background:0 0;outline:none;border:none}p:not(:last-child){margin-bottom:20px}h1,h2,h3,h4,h5,h6,span,footer,p,ol,li{font-family:roboto,sans-serif}h1{font-size:72px;font-weight:500}@media(max-width:600px){h1{font-size:40px;line-height:53px}}h2{font-size:56px;line-height:56px;font-weight:500;margin-bottom:15px}@media(max-width:600px){h2{font-size:36px;line-height:35px}}h3{font-size:36px;font-weight:500}@media(max-width:600px){h3{font-size:21px;line-height:28px}}h4{font-size:24px;line-height:28px;font-weight:500}@media(max-width:600px){h4{font-size:21px;line-height:26px}}h5{font-size:17px;font-weight:400}@media(max-width:600px){h5{line-height:26px}}h6{font-size:12px;font-weight:400}@media(max-width:600px){h6{line-height:26px}}p{font-size:21px;font-weight:400;line-height:39px}@media(max-width:600px){p{font-size:17px;line-height:34px}}ol,ul{line-height:39px}li{font-size:21px}.project-type{color:var(--color-tertiary);font-size:14px}.link,.link--view-project{cursor:pointer;width:max-content}.link--view-project{color:var(--color-link);font-size:17px;display:flex}.link--view-project :hover{transform:translateX(5px)}.link--view-project svg{transition:all .3s;margin-left:5px;width:24px;height:24px;fill:currentColor}.icon,.icon--social{cursor:pointer;display:block;fill:#000}.icon--social{height:48px;width:48px}.external-link-icon{position:absolute;top:14px;right:35px;top:17px;right:17px;fill:#8bc34a;width:24px;height:24px;cursor:pointer;transition:all .3s}.external-link-icon:hover{fill:blue}footer{text-align:center;padding:30px 0}nav{display:flex;justify-content:space-between;padding-top:var(--space-md);margin:0 auto;max-width:80vw;color:#fff;position:sticky}.nav-links{display:flex;margin:0 -10px}.nav-links li>*{margin:0 10px}body{background-color:var(--color-bg)}.container{display:grid;gap:100px}@media(max-width:600px){.container{gap:20px}}.hero{display:flex;flex-direction:column;align-items:center;justify-content:center;padding:0 50px}@media(max-width:600px){.hero{padding:20px;background:#fff;text-align:center}}.hero--img{margin-top:40px;max-width:1000px}.case-studies{display:grid;gap:200px;background-color:#fff;padding:100px 0}@media(max-width:992px){.case-studies{gap:100px;padding:20px}}@media(max-width:600px){.case-studies{gap:50px}}.case-studies--title{text-align:center}.case-studies .case-study{display:flex;max-width:1200px;margin:0 auto;flex-wrap:wrap;justify-content:center}@media(max-width:992px){.case-studies .case-study{flex-direction:column-reverse;text-align:center}.case-studies .case-study>*{margin-bottom:30px}}.case-studies .case-study__text{max-width:400px;display:grid;gap:15px;justify-content:center}@media(max-width:992px){.case-studies .case-study__text{justify-items:center;margin:0 auto;max-width:100%;gap:10px}}.case-studies .case-study__text__tags{display:flex;color:var(--color-gray)}.case-studies .case-study__text__tags--tag:not(:last-child){margin-right:15px}.case-studies .case-study--img{max-width:420px;height:auto}@media(max-width:992px){.case-studies .case-study--img{max-width:100%}}img{width:100%;height:auto}.reverse{flex-direction:row-reverse}.notable-projects{justify-content:center;background-color:#fff;margin:100px;padding:100px;text-align:center}@media(max-width:992px){.notable-projects{margin:50px;padding:50px}}@media(max-width:600px){.notable-projects{padding:20px;margin:0}}.notable-projects__grid{margin-top:100px;display:grid;gap:60px;grid-template-columns:repeat(3,1fr)}@media(max-width:600px){.notable-projects__grid{margin-top:30px}}@media(max-width:992px){.notable-projects__grid{grid-template-columns:1fr}}.notable-projects__grid__project__image{position:relative}.notable-projects__grid__project--title{margin-top:10px}.facts{display:grid;gap:100px;grid-template-columns:repeat(3,1fr);max-width:1200px;margin:0 auto;padding:20px}@media(max-width:992px){.facts{grid-template-columns:1fr;gap:50px}}@media(max-width:600px){.facts{background-color:#fff}}.facts__fact{display:flex;flex-direction:column;align-items:center;text-align:center}.facts__fact *:not(:last-child){margin-bottom:10px}.facts__fact--icon{width:50px;height:auto}.facts__fact__tags{display:flex;flex-wrap:wrap;color:var(--color-gray);margin-top:10px}.facts__fact__tags--tag:not(:last-child){margin-right:15px}.about--title{text-align:center}.about__grid{columns:2 auto;column-gap:50px;margin:100px auto;max-width:1200px;padding:20px}@media(max-width:992px){.about__grid{columns:1 auto;margin:50px}}@media(max-width:600px){.about__grid{columns:1 auto;margin:0;padding:0}}.about__grid>*{background-color:#fff;padding:44px;display:inline-block;margin:30px auto;width:100%}@media(max-width:600px){.about__grid>*{padding:22px;margin:10px 0}}.about__grid__img{text-align:center}.about__grid__skills{text-align:center}.about__grid__skills__list{display:flex;flex-wrap:wrap;justify-content:center;margin-top:25px}.about__grid__skills__list>*:not(:last-child){margin-right:20px}.about__grid__socials{width:100%;text-align:center}.about__grid__socials__icons{display:flex;justify-content:space-around}.about__grid__socials__icons :hover>*{fill:red;transition:all .3s}